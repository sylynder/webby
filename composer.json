{
    "name": "sylynder/webby",
    "type": "project",
    "description": "Webby: A lego-like PHP Framework extended from the CodeIgniter3 Framework for easy web application development",
    "keywords": [
        "framework",
        "sylynder",
        "webby",
        "codeigniter",
        "php"
    ],
    "license": "MIT",
    "authors": [{
        "name": "Kwame Oteng Appiah-Nti",
        "email": "developerkwame@gmail.com"
    }],
    "require": {
        "php": "^8.0",
        "sylynder/codeigniter": "^0.3"
    },
    "require-dev": {
        "symfony/var-dumper": "^5.2"
    },
    "config": {
        "optimize-autoloader": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "app",
            "Base\\": "engine/Core/core"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "@php webby set --env",
            "@php webby key:generate"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
